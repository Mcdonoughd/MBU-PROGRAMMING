#ifndef LIST_H_
#define LIST_H_
class Node
{
public:
    Node* next;
    int data;
};

using namespace std;

class LinkedList
{
public:
    int length;
    Node* head;

    LinkedList();
    ~LinkedList();
    void add(int data);
    void print();
};
#endif /* LIST_H_ */
