#ifndef STACK_H_
#define STACK_H_
#include <iostream>
#include <cstdlib>
using namespace std;

// Define node pointer
class Node {
public:
    int data;
    Node* next;
};



class Queue{
public:
    int length; //counter for the length of a list
    Node* head;
    Node* tail;
//define the functions to be done on a list
    Stack();
    ~Stack();
    void print();
    void push(int data);
    void pop();
    void top();
    bool isEmpty();
};
// Head and tail pointers



#endif //STACK_H
